{
  "name": "openrpc-docs",
  "version": "1.0.0",
  "type": "module",
  "description": "Interactive documentation generator for OpenRPC JSON-RPC APIs",
  "main": "dist/openrpc-docs.umd.js",
  "module": "dist/openrpc-docs.es.js",
  "unpkg": "dist/openrpc-docs.umd.js",
  "jsdelivr": "dist/openrpc-docs.umd.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:types",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit"
  },
  "keywords": [
    "openrpc",
    "json-rpc",
    "documentation",
    "interactive",
    "vue",
    "tailwind"
  ],
  "author": "Vladyslav Chaliuk <chaliukvladyslav@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^14.2.0",
    "@nuxt/ui": "^3.3.2",
    "vue": "3.5.20",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@open-rpc/meta-schema": "^1.14.9",
    "@types/node": "24.3.0",
    "@vitejs/plugin-vue": "6.0.1",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/tsconfig": "0.8.1",
    "autoprefixer": "^10.4.15",
    "eslint": "9.34.0",
    "eslint-plugin-vue": "10.4.0",
    "typescript": "5.9.2",
    "vite": "7.1.3",
    "vue-router": "^4.5.0",
    "vue-tsc": "3.0.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AstralMortem/openrpc-docs.git"
  },
  "homepage": "https://github.com/AstralMortem/openrpc-docs#readme"
}
